#include <bits/stdc++.h>
#define MAX_MASK 1024
#define MAX_N 1010
#define FOR(i,a,b) for(int i=a;i<b;i++)
using namespace std;

vector<pair<int,int> > adj[MAX_N];
int mask[MAX_MASK];

int main(){
  ios::sync_with_stdio(false);
  int n,m,k;
  cin>>n>>m>>k;
  FOR(i,0,n){
    int t;
    cin>>t;
    FOR(j,0,t){
      int msk;
      cin>>msk;
      mask[i] |= msk;
    }
  }
  
  
  return 0;
}
