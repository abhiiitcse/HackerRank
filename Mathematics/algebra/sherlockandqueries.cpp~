#include <iostream>
#include <vector>
using namespace std;
typedef long long ll;
int main(){
  ll n,m;
  cin>>n>>m;
  vector<ll> arra(n);
  vector<ll> arrb(m);
  vector<ll> arrc(m);
  for(int i=0;i<n;i++){
    cin>>arra[i];
  }
  for(int i=0;i<m;i++){
    cin>>arrb[i];
  }
  for(int i=0;i<m;i++){
    cin>>arrc[i];
  }
  
  return 0;
}
